{
  "name": "specjet",
  "version": "0.2.7",
  "description": "API contract collaboration tool - Design APIs together, build separately, integrate seamlessly",
  "main": "src/index.js",
  "bin": {
    "specjet": "./bin/specjet"
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:performance": "RUN_PERFORMANCE_TESTS=true vitest run tests/performance/",
    "test:stress": "RUN_STRESS_TESTS=true vitest run tests/performance/stress-test.test.js",
    "test:all": "RUN_PERFORMANCE_TESTS=true RUN_STRESS_TESTS=true vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev": "node bin/specjet",
    "audit": "npm audit --audit-level high",
    "audit:fix": "npm audit fix --audit-level high",
    "security:check": "npm audit && npm outdated"
  },
  "keywords": [
    "api",
    "openapi",
    "swagger",
    "typescript",
    "mock",
    "cli",
    "codegen",
    "contract"
  ],
  "author": "Kyle Logue",
  "license": "MIT",
  "files": [
    "src/",
    "bin/",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@faker-js/faker": "^9.2.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "commander": "^12.1.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "fs-extra": "^11.2.0",
    "handlebars": "^4.7.8",
    "js-yaml": "^4.1.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@vitest/ui": "^2.1.8",
    "eslint": "^9.17.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.5.2",
    "typescript": "^5.9.2",
    "vitest": "^2.1.8"
  },
  "type": "module",
  "imports": {
    "#src/*": "./src/*"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/specjet-api/specjet"
  },
  "bugs": {
    "url": "https://github.com/specjet-api/specjet/issues"
  },
  "homepage": "https://specjet.dev",
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  }
}
